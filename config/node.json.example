{
  "forward_stats": false,
  "startup_wait": 10,
  "chain": {
    "enabled": true,
    "trail_head_blocks": 1,
    "rpc_url": "$RPC_URL",
    "registry_address": "$REGISTRY",
    "wallet": {
      "max_gas_limit": 5000000,
      "private_key": "$PRIVATE_KEY"
    },
    "snapshot_sync": {
      "sleep": 4,
      "batch_size": 1000,
      "starting_sub_id": $STARTING_SUB_ID
    }
  },
  "server": {
    "port": 4000
  },
  "docker": {
    "username": "username",
    "password": "password"
  },
  "redis": {
    "host": "redis",
    "port": 6379
  },
  "containers": [
    {
      "id": "hello-world",
      "image": "lagunovsky/ritualnetwork-infernet-worker-gm:latest",
      "external": true,
      "port": "3000",
      "allowed_delegate_addresses": [],
      "allowed_addresses": [],
      "allowed_ips": [],
      "command": "--bind=0.0.0.0:3000 --workers=4",
      "env": {},
      "volumes": [],
      "generates_proofs": false,
      "accepted_payments": {}
    }
  ]
}
